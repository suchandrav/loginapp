r<?xml version='1.0' encoding='UTF-8'?>
<box type="box">
    <description>LoginApp: Automated cucumber tests</description>
    <name>app0206.be.dev</name>
    <username>b2b</username>
    <container>/tmp/autob</container>
    <assets>
